  "animations": {
    "duration": {
      "fast": "0.15s",
      "normal": "0.3s",
      "slow": "0.5s",
      "slower": "0.85s"
    },
    "easing": {
      "ease": "ease",
      "easeIn": "ease-in",
      "easeOut": "ease-out",
      "easeInOut": "ease-in-out",
      "bounce": "cubic-bezier(0.16, 1, 0.3, 1)"
    },
    "transitions": {
      "all": "all 0.3s ease-in-out",
      "transform": "transform 0.3s ease",
      "opacity": "opacity 0.3s ease",
      "background": "background-color 0.3s ease",
      "color": "color 0.3s ease",
      "border": "border-color 0.3s ease",
      "shadow": "box-shadow 0.3s ease"
    },
    "keyframes": {
      "ripple": {
        "0%": { "boxShadow": "0 0 0 .1rem rgba(0,0,0, 0.08)" },
        "100%": { "boxShadow": "0 0 0 3rem rgba(0,0,0, 0)" }
      },
      "rippleTwo": {
        "0%": { "boxShadow": "0 0 0 .1rem rgba(255, 145, 77, 0.15)" },
        "100%": { "boxShadow": "0 0 0 0.8rem rgba(255, 145, 77, 0.15)" }
      },
      "rotated": {
        "0%": { "transform": "rotate(0)" },
        "100%": { "transform": "rotate(360deg)" }
      },
      "jump": {
        "0%": { "transform": "translate3d(0,0,0)" },
        "40%": { "transform": "translate3d(0,50%,0)" },
        "100%": { "transform": "translate3d(0,0,0)" }
      },
      "fadeIn": {
        "0%": { "opacity": 0 },
        "100%": { "opacity": 1 }
      },
      "slideUp": {
        "0%": { "transform": "translateY(20px)", "opacity": 0 },
        "100%": { "transform": "translateY(0)", "opacity": 1 }
      },
      "scaleIn": {
        "0%": { "transform": "scale(0.9)", "opacity": 0 },
        "100%": { "transform": "scale(1)", "opacity": 1 }
      }
    }
  },

